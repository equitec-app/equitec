<div class="modal fade" id="chooseHeadquarterModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title azul-oscuro" id="exampleModalLabel">Selecciona una sede</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
        <div class="modal-body">
          <%@headquarters.each do |headquarter|%>
          <%=headquarter.city%>, <%=headquarter.direction%> <br>

          <%end%>
        </div>
    </div>
  </div>
</div>
